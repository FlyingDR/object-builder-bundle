{
    "name": "flying/object-builder-bundle",
    "description": "Symfony bundle to integrate object builder",
    "authors": [
        {
            "name": "Alexander Grimalovsky",
            "email": "alexander.grimalovsky@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Flying\\ObjectBuilderBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Flying\\ObjectBuilderBundle\\Tests\\": "Tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/http-kernel": "^3.3|^4.0",
        "symfony/dependency-injection": "^3.3|^4.0",
        "symfony/config": "^3.3|^4.0",
        "symfony/finder": "^3.3|^4.0",
        "flying/object-builder": "^0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "roave/security-advisories": "dev-master"
    }
}
