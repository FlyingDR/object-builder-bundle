{
    "name": "flying/object-builder-bundle",
    "description": "Symfony bundle to integrate object builder",
    "authors": [
        {
            "name": "Alexander Grimalovsky",
            "email": "alexander.grimalovsky@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Flying\\ObjectBuilderBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Flying\\ObjectBuilderBundle\\Tests\\": "Tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "roave/security-advisories": "dev-master",
        "symfony/http-kernel": "^3.3",
        "symfony/dependency-injection": "^3.3",
        "flying/object-builder": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    }
}
